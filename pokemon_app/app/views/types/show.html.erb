<% @page_title = "Show Type" %>

<%= link_to("<< Back to List", types_path(:page => params[:page])) %>

<div>
  <h2>Show Type</h2>
  <table>
    <tr>
      <th>#</th>
      <td><%= @type.id %></td>
    </tr>
    <tr>
      <th>Name</th>
      <td><%= @type.name %></td>
    </tr>
    <tr>
      <th>url</th>
      <td><%= link_to(@type.url, @type.url) %></td>
    </tr>
    <tr>
      <th>created at</th>
      <td><%= @type.created_at %></td>
    </tr>
    <tr>
      <th>updated at</th>
      <td><%= @type.updated_at %></td>
    </tr>
  </table>

</div>